    private void radBoldItemStateChanged(java.awt.event.ItemEvent evt) {                                         
        if(radBold.isSelected()){
            java.awt.Font oldFont = this.getFont();
        java.awt.Font newFont = new java.awt.Font(oldFont.getName(),
        2, oldFont.getSize()); 
        getContentPane().setFont(newFont);
        }
    }                                        

