    private void radBoldActionPerformed(java.awt.event.ActionEvent evt) {                                        
        java.awt.Font oldFont = this.getFont();
        java.awt.Font newFont = new java.awt.Font(oldFont.getName(),
        1, oldFont.getSize());              
        
            this.setFont(newFont);
            this.getContentPane().setFont(newFont);
                      
    }                                       

    private void btn1ActionPerformed(java.awt.event.ActionEvent evt) {                                     
        String num = "1";      
        if(txtInput.getText().equals("0.0")){              
            txtInput.setText(String.valueOf(num));
        }
        else{
           
            txtInput.setText(txtInput.getText() + String.valueOf(num));
        }       
        
    }                                    

