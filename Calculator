    public double num1 = 0;
    public double num2 = 0;
    public double answer = 0;
    public char operator;
        
    void round(){
        if(radRound1.isSelected()){
            num1 = Math.round(num1 *10);
        }
            else if(radRound2.isSelected()){
                    num1 = Math.round(num1 *100);
            }
            else if(radRound3.isSelected()){
                num1 = Math.round(num1 *1000);
            }
            else if(radRound4.isSelected()){
                num1 = Math.round(num1 *10000);
            }
            else if(radRound5.isSelected()){
                num1 = Math.round(num1 *100000);
            }
            else if(radRound6.isSelected()){
                num1 = Math.round(num1 *1000000);                                   
            }
            else if(radRound7.isSelected()){
                num1 = Math.round(num1 *10000000);
            }
            else if(radRound8.isSelected()){
                num1 = Math.round(num1 *100000000);
            }
            else if(radRound9.isSelected()){
                num1 = Math.round(num1 *100000000);
            }
    }
    
    void calculate(){
        switch(operator){
            case '+':                 
                num2 = Double.parseDouble(txtInput.getText());                
                num1 += num2;
                txtInput.setText("" + num1);
                break;
            case '-':
                num2 = Double.parseDouble(txtInput.getText());
                num1 -= num2;
                txtInput.setText("" + num1);
                break;
            case '*':
                num2 = Double.parseDouble(txtInput.getText());
                num1 *= num2;
                txtInput.setText("" + num1);
                break;
            case '/':
                num2 = Double.parseDouble(txtInput.getText());
                if(num2==0){
                    msgDivide.showMessageDialog(this, "ERROR: Divide by zero", "Message", 
                            javax.swing.JOptionPane.ERROR_MESSAGE);
                           }
                else{                    
                    num1 /= num2;
                    txtInput.setText("" + num1);
                }                  
                break;
            case '^':
                num2 = Double.parseDouble(txtInput.getText());
                //num1 = num1 * num1 * num2;
                num1 = Math.pow(num1, num2);
                txtInput.setText("" + num1);
                
        }
    }

