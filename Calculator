    private void mnuHotKeysActionPerformed(java.awt.event.ActionEvent evt) {                                           
        msgHelp.showMessageDialog(this, "+ - addition\n"
                + "- - subtraction\n"
                + "* - multiplication\n"
                + "\\ - division\n\n"
                + "s - sin\n"
                + "S - arcsin\n"
                + "c - cos\n"
                + "C - arccos\n"
                + "t - tan\n"
                + "T - arctan\n\n"
                + "i - inverse\n"
                + "m - +/-\n"
                + "^ - power\n"
                + "! - factorial\n\n"
                + "d - clear all(CA)\n"
                + "D - clear entry(CE)\n\n"
                + "Digits 0 - 9 can be entered from the\n"
                + "keypad or the keyboard's digits", "Hotkeys", javax.swing.JOptionPane.INFORMATION_MESSAGE);
    }                                          

    private void mnuAboutActionPerformed(java.awt.event.ActionEvent evt) {                                         
        msgAbout.showMessageDialog(this, "CpCalculator\n"
                + "Version 1.2.3_456\n"
                + "April 2015\n\n"
                + "Created by Ash Swinburne", "Message", javax.swing.JOptionPane.INFORMATION_MESSAGE);
    }                                        

    private void btnCAActionPerformed(java.awt.event.ActionEvent evt) {                                      
        txtInput.removeAll();
        txtInput.setText(null);        
    }                                     

    private void btnEqualsActionPerformed(java.awt.event.ActionEvent evt) {                                          
        String Input = String.valueOf(num1);
        String Input2 = String.valueOf(num2);

        calculate();
        num1 = 0;
        num2 = 0;     
        
        
    }                                         

    private void btnMinusActionPerformed(java.awt.event.ActionEvent evt) {                                         
        num1 = Double.parseDouble(txtInput.getText());
        operator = '-';
        txtInput.setText("");
    }                                        

    private void btnMultiplyActionPerformed(java.awt.event.ActionEvent evt) {                                            
        num1 = Double.parseDouble(txtInput.getText());
        operator = '*';
        txtInput.setText("");
    }                                           

    private void btnDivideActionPerformed(java.awt.event.ActionEvent evt) {                                          
        num1 = Double.parseDouble(txtInput.getText());
        operator = '/';
        txtInput.setText("");
    }                                         

    private void btnDecimalActionPerformed(java.awt.event.ActionEvent evt) {                                           
        
        String Input = txtInput.getText();
        String num = ".";
        if(txtInput.getText().equals("0.0")){              
            txtInput.setText(String.valueOf(num));
        }
        else{
            
            txtInput.setText(txtInput.getText() + String.valueOf(num));
        }
        //double check if can have two ifs
        if(Input.contains(num)){
            btnDecimal.disable();
        }
        else{
        txtInput.setText(Input + num);
        }
        
    }                                          

    private void btnReciprocalActionPerformed(java.awt.event.ActionEvent evt) {                                              
        num1 = 1;        
        num2 = Double.parseDouble(txtInput.getText());
        answer = 0;
        
        if(num2 == 0){
            msgReciprocal.showMessageDialog(this, "ERROR: Inverse of zero is infinity", "Message"
            , javax.swing.JOptionPane.ERROR_MESSAGE);
        }
        else{
        answer = num1/num2;
        txtInput.setText(String.valueOf(answer));
        }
    }                                             

    private void btnPowerOfActionPerformed(java.awt.event.ActionEvent evt) {                                           
        num1 = Double.parseDouble(txtInput.getText());
        operator = '^';
        txtInput.setText("");
    }                                          

    private void btnInverseActionPerformed(java.awt.event.ActionEvent evt) {                                           
        
        double Input = Double.parseDouble(txtInput.getText());        
        Input -= Input*2;
        
        txtInput.setText(String.valueOf(Input));
    }                                          

    private void btnFactorialActionPerformed(java.awt.event.ActionEvent evt) {                                             
        double counter = Double.parseDouble(txtInput.getText());
        double counter2 = Double.parseDouble(txtInput.getText())-1;
         
        System.out.println(counter);
        System.out.println(counter2);
          if(counter<0||counter>20){
            msgFactorial.showMessageDialog(this, "ERROR: Must be a positive integer between 0 and 20",
                    "Message", javax.swing.JOptionPane.ERROR_MESSAGE);
          }
          else if(counter == 0){
              counter = 1;
          }
            else {
              for(; counter2>1; counter2--){            
            counter = counter * counter2;
              }             
          }         
        
        txtInput.setText(String.valueOf(counter));
    }                                            

    private void btnPIActionPerformed(java.awt.event.ActionEvent evt) {                                      
        num1 = Math.PI;
        txtInput.setText(String.valueOf(num1));
    }                                     

    private void btnRadRadiansActionPerformed(java.awt.event.ActionEvent evt) {                                              
        Math.toRadians(Double.parseDouble(txtInput.getText()));        
    }                                             

    private void btnEActionPerformed(java.awt.event.ActionEvent evt) {                                     
        num1 = Math.E;
        txtInput.setText(String.valueOf(num1));
    }                                    

    private void btnSinActionPerformed(java.awt.event.ActionEvent evt) {                                       
        if(btnRadDegrees.isSelected()){
        num1 = Math.sin(Math.toRadians(Double.parseDouble(txtInput.getText())));
        txtInput.setText(String.valueOf(num1));         
        }
        else{
            num1 = Math.sin(Double.parseDouble(txtInput.getText()));
            txtInput.setText(String.valueOf(num1));  
        }
    }                                      

    private void btnRadDegreesActionPerformed(java.awt.event.ActionEvent evt) {                                              
        Math.toDegrees(Double.parseDouble(txtInput.getText()));
    }                                             

    private void btnAsinActionPerformed(java.awt.event.ActionEvent evt) {                                        

        if(btnRadDegrees.isSelected()){
            num1 = Double.parseDouble(txtInput.getText());
                   num2 = Math.toDegrees(Math.asin(num1));
                    txtInput.setText(String.valueOf(num2));
        }
        else{
            num1 = Double.parseDouble(txtInput.getText());
                   num2 = Math.toRadians(Math.asin(num1));
                    txtInput.setText(String.valueOf(num2));
        }
    }                                       

    private void btnCosActionPerformed(java.awt.event.ActionEvent evt) {                                       
        if(btnRadDegrees.isSelected()){
        num1 = Math.cos(Math.toRadians(Double.parseDouble(txtInput.getText())));
        txtInput.setText(String.valueOf(num1));         
        }
        else{ 
            num1 = Math.cos(Double.parseDouble(txtInput.getText()));
            txtInput.setText(String.valueOf(num1));  
        }
    }                                      

    private void btnAcosActionPerformed(java.awt.event.ActionEvent evt) {                                        
        if(btnRadDegrees.isSelected()){
            num1 = Double.parseDouble(txtInput.getText());
                   num2 = Math.toDegrees(Math.acos(num1));
                    txtInput.setText(String.valueOf(num2));
        }
        else{
            num1 = Double.parseDouble(txtInput.getText());
                   num2 = Math.toRadians(Math.acos(num1));
                    txtInput.setText(String.valueOf(num2));
        }
    }                                       

    private void btnTanActionPerformed(java.awt.event.ActionEvent evt) {                                       
        if(btnRadRadians.isSelected()){
            num1 = Math.tan(Double.parseDouble(txtInput.getText()));
            txtInput.setText(String.valueOf(num1));         
        }
        else{            
            num1 = Math.tan(Math.toRadians(Double.parseDouble(txtInput.getText())));
            txtInput.setText(String.valueOf(num1));  
        }
    }                                      

    private void btnAtanActionPerformed(java.awt.event.ActionEvent evt) {                                        
